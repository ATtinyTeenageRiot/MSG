  pot1Raw = analogRead(A1); // Read Pin 1 as analog input
  pot2Raw = analogRead(A2); // Read pin 2 as analog input
  pot3Raw = analogRead(A3); // Read pin 3 as analog input
  
  if (trigState == LOW) 
  {
  loopSize = map(pot1Raw, 1, 1023, 20, 200);
  }
  else 
  {
  loopSize = map(pot1Raw, 1, 1023, file_len, 20);
  }

  select = map(pot3Raw, 1, 1023, 1, 16);
  
  pitch = map(pot2Raw, 1, 1023, 200, 15);

  OCR0A = pitch;
  
}

// Sample interrupt 
ISR(TIMER0_COMPA_vect) {
  
   if (trigState == LOW)
   {
     p--;
     
   if (p <= loopSize)
    {
    p = file_len;
    } 
     
   }

   else
   {
    p++; 
   
    if (p >= loopSize)
    {
    p = 0;
    }   
    
   }
   
    switch (select)
    {
    case 1:
      sample = pgm_read_byte(&msgOne_wav[p]);
      file_len = msgOne_wav_len;
      break;

    case 2:
      sample = pgm_read_byte(&msgTwo_wav[p]);
      file_len = msgTwo_wav_len;
      break;

    case 3:
      sample = pgm_read_byte(&msgThree_wav[p]);
      file_len = msgThree_wav_len;
      break;

    case 4:
      sample = pgm_read_byte(&msgFour_wav[p]);
      file_len = msgFour_wav_len;
      break;
      
    case 5:
      sample = pgm_read_byte(&msgFive_wav[p]);
      file_len = msgFive_wav_len;
      break;

    case 6:
      sample = pgm_read_byte(&msgSix_wav[p]);
      file_len = msgSix_wav_len;
      break;

    case 7:
      sample = pgm_read_byte(&msgSeven_wav[p]);
      file_len = msgSeven_wav_len;
      break;

    case 8:
      sample = pgm_read_byte(&msgEight_wav[p]);
      file_len = msgEight_wav_len;
      break;

    case 9:
      sample = pgm_read_byte(&msgNine_wav[p]);
      file_len = msgNine_wav_len;
      break;

    case 10:
      sample = pgm_read_byte(&msgTen_wav[p]);
      file_len = msgTen_wav_len;
      break;
      
    case 11:
      sample = pgm_read_byte(&msgEleven_wav[p]);
      file_len = msgEleven_wav_len;
      break;

    case 12:
      sample = pgm_read_byte(&msgTwelve_wav[p]);
      file_len = msgTwelve_wav_len;
      break;

    case 13:
      sample = pgm_read_byte(&msgThirteen_wav[p]);
      file_len = msgThirteen_wav_len;
      break;

    case 14:
      sample = pgm_read_byte(&msgFourteen_wav[p]);
      file_len = msgFourteen_wav_len;
      break;
      
    case 15:
      sample = pgm_read_byte(&msgFifteen_wav[p]);
      file_len = msgFifteen_wav_len;
      break;

    case 16:
      sample = pgm_read_byte(&msgSixteen_wav[p]);
      file_len = msgSixteen_wav_len;
      break;
      } 
      
    OCR1A = sample;

}
